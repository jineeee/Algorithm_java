package practice;

/*
 */

public class SkillCheck_1_4 {
	
	static int n=5;
	static int[] a1 = {9, 20, 28, 18, 11};
	static int[] a2 = {30, 1, 21, 17, 28};

	static public String[] solution(int[] arr1, int[] arr2) {
		 String[] answer = {};
		 
		 String[] map = new String[n];
	        for(int i = 0 ; i < n ; i++) {
	            map[i] = Integer.toBinaryString(arr1[i] | arr2[i]);
	    }
	      
	      
	      for (int i=0; i<n; i++) {
	    	  map[i] = map[i].replace("1", "#");
	    	  map[i] = map[i].replace("0", " ");
	      }
	      
	      for(int i=0; i<n; i++) {
	    	  System.out.println(map[i]);
	      }
	      
	      return answer;
	  }
	
	public static void main(String[] args) {
		System.out.println(solution(a1, a2));
	}
}
